<?xml version="1.0" encoding="UTF-8"?>  
<web-app>
	<servlet>
	 <servlet-name>login</servlet-name>
	 <servlet-class>com.study.user.LoginServlet</servlet-class>
	</servlet>
	<servlet>
	 <servlet-name>reg</servlet-name>
	 <servlet-class>com.study.user.RegisterServlet</servlet-class>
	</servlet>   
	<servlet-mapping>
	 <servlet-name>login</servlet-name>
	 <url-pattern>/login</url-pattern> 
	</servlet-mapping>  
	<servlet-mapping>
	 <servlet-name>reg</servlet-name>
	 <url-pattern>/reg</url-pattern> 
	</servlet-mapping>
</web-app>
